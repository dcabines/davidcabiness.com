    --color-dimmed3: #727072;
    --color-dimmed4: #5b595c;
    --color-dimmed5: #403e41;
  }

  .code-block .line {
    counter-increment: line-number;
    margin-left: -2ch;
  }

  .code-block .line:before {
    display: inline-block;
    content: counter(line-number);
    color: var(--color-dimmed4);
    font-size: 0.8em;
    margin-right: 3ch;
    width: 3ch;
    text-align: right;
  }

  .code-block .line.indent:before {
    margin-right: 5ch;
  }

  .code-block .line.indent-2:before {
    margin-right: 7ch;
  }

  .code-block .line .word span:not(:first-child) {
    margin-left: -1ch;
  }

  html,
  body {
    margin: 0;
    padding: 0;
    overflow: hidden;
    font-family: "Lucida Console", Monaco, monospace;
    background: linear-gradient(135deg, #ffe0e3 32%, #a7e0e5 68%);
    height: 100vh;
  }

  .content {
    display: flex;
    align-items: center;
    flex-direction: column;
    gap: 40px;
    padding-top: 100px;
  }

  .card {
    perspective: 150rem;
    width: 32rem;
    height: 10rem;
    position: relative;
  }

  .card-side {
    position: absolute;
    top: 0;
    left: 0;
    padding: 1rem;
    height: 8rem;
    width: 30rem;
    overflow: hidden;
    backface-visibility: hidden;
    border-radius: 10px;
    box-shadow: 0 2rem 6rem rgba(0, 0, 0, 0.6);
    transition: all 0.8s ease;
  }

  .card-front {
    background-color: #1c1c1c;
  }

  .card-back {
    background-color: #1c1c1c;
    transform: rotateY(180deg);
  }

  .card:hover .card-front {
    transform: rotateY(-180deg);
  }

  .card:hover .card-back {
    transform: rotateY(0deg);
  }
</style>
